@article{Gonzalez-Parra2011,
Author = {Gonzalez-Parra, Paula A. and Lee, Sunmi and Velazquez, Leticia and
   Castillo-Chavez, Carlos},
Title = {A Note on the Use of Optimal Control on a Discrete Time Model of Influenza Dynamics},
Journal = {Mathematical Biosciences and Engineering},
Year = {{2011}},
Volume = {{8}},
Number = {{1}},
Pages = {{183-197}},
Month = {January},
Abstract = {{A discrete time Susceptible - Asymptomatic - Infectious - Treated -
   Recovered (SAITR) model is introduced in the context of influenza
   transmission. We evaluate the potential effect of control measures such
   as social distancing and antiviral treatment on the dynamics of a single
   outbreak. Optimal control theory is applied to identify the best way of
   reducing morbidity and mortality at a minimal cost. The problem is
   solved by using a discrete version of Pontryagin's maximum principle.
   Numerical results show that dual strategies have stronger impact in the
   reduction of the final epidemic size.}},
