Title = {{Comparison of optimal controls for a model in cancer chemotherapy with
   L(1)- and L(2)-type objectives}},
Journal = {Optimization Methods \& Software},
Year = {{2004}},
Volume = {{19}},
Number = {{3-4}},
Pages = {{339-350}},
Month = {June--August},
